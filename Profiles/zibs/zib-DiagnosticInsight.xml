<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="zib-DiagnosticInsight" />
  <url value="http://nictiz.nl/fhir/StructureDefinition/zib-DiagnosticInsight" />
  <name value="ZibDiagnosticInsight" />
  <title value="zib DiagnosticInsight" />
  <status value="draft" />
  <publisher value="Nictiz" />
  <contact>
    <name value="Nictiz" />
    <telecom>
      <system value="url" />
      <value value="https://www.nictiz.nl" />
      <use value="work" />
    </telecom>
  </contact>
  <description value="Diagnostic insight is the interpretation of the condition by the care provider. This is based on known combinations of conditions and the symptoms with which these present themselves. It may involve a single diagnosis or a differential diagnosis with conditions that are under consideration." />
  <purpose value="This Condition resource represents the Dutch [zib ('Zorginformatiebouwsteen', i.e. Health and Care Information Model) DiagnosticInsight v1.0(2023)](https://zibs.nl/wiki/DiagnosticInsight-v1.0(2023EN))." />
  <copyright value="Copyright and related rights waived via CC0, https://creativecommons.org/publicdomain/zero/1.0/. This does not apply to information from third parties, for example a medical terminology system. The implementer alone is responsible for identifying and obtaining any necessary licenses or authorizations to utilize third party IP in connection with the specification or otherwise." />
  <fhirVersion value="4.0.1" />
  <mapping>
    <identity value="zib-diagnosticinsight-v1.0-2023EN" />
    <uri value="https://zibs.nl/wiki/DiagnosticInsight-v1.0(2023EN)" />
    <name value="zib DiagnosticInsight-v1.0(2023EN)" />
  </mapping>
  <kind value="resource" />
  <abstract value="false" />
  <type value="Condition" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Condition" />
  <derivation value="constraint" />
  <differential>
    <element id="Condition">
      <path value="Condition" />
      <short value="DiagnosticInsight" />
      <alias value="DiagnostischInzicht" />
      <alias value="zib-DiagnosticInsight" />
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.1" />
        <comment value="DiagnosticInsight" />
      </mapping>
    </element>
    <element id="Condition.extension:diagnosticInsightDate">
      <path value="Condition.extension" />
      <sliceName value="diagnosticInsightDate" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org/fhir/StructureDefinition/condition-assertedDate" />
      </type>
    </element>
    <element id="Condition.extension:diagnosticInsightDate.value[x]">
      <path value="Condition.extension.value[x]" />
      <short value="DiagnosticInsightDate" />
      <definition value="Date (and time) at which the care professional obtained the diagnostic insight." />
      <alias value="DiagnostischInzichtDatum" />
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.12" />
        <comment value="DiagnosticInsightDate" />
      </mapping>
    </element>
    <element id="Condition.extension:condition">
      <path value="Condition.extension" />
      <sliceName value="condition" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org/fhir/StructureDefinition/condition-related" />
      </type>
    </element>
    <element id="Condition.extension:condition.value[x]">
      <path value="Condition.extension.value[x]" />
      <short value="Condition" />
      <definition value="The Condition to which the DiagnosticInsight applies." />
      <alias value="Aandoening" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Condition" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/zib-Condition" />
      </type>
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.16" />
        <comment value="Condition" />
      </mapping>
    </element>
    <element id="Condition.extension:dueTo">
      <path value="Condition.extension" />
      <sliceName value="dueTo" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://hl7.org/fhir/StructureDefinition/condition-dueTo" />
      </type>
    </element>
    <element id="Condition.extension:dueTo.value[x]">
      <path value="Condition.extension.value[x]" />
      <short value="RelatedTo" />
      <alias value="Aanleiding" />
      <type>
        <code value="CodeableConcept" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Condition" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Procedure" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/MedicationAdministration" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Immunization" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/MedicationStatement" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/zib-DiagnosticInsight" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/zib-Procedure-event" />
      </type>
      <binding>
        <strength value="extensible" />
        <valueSet value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.40.2.99.99.12--20240101000000" />
      </binding>
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.21" />
        <comment value="RelatedTo" />
      </mapping>
    </element>
    <element id="Condition.extension:methodOfConfirmation">
      <path value="Condition.extension" />
      <sliceName value="methodOfConfirmation" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://nictiz.nl/fhir/StructureDefinition/ext-DiagnosticInsight.MethodOfConfirmation" />
      </type>
    </element>
    <element id="Condition.verificationStatus">
      <path value="Condition.verificationStatus" />
      <short value="DegreeOfCertainty" />
      <definition value="Indicates the conviction of the care professional with respect to the diagnostic insight as interpretation of the condition." />
      <alias value="ZekerheidsStatus" />
      <binding>
        <strength value="required" />
        <description value="Use ConceptMap ZekerheidsStatusCodelijst_to_ConditionVerificationStatus to translate terminology from the functional model to profile terminology in ValueSet ConditionClinicalStatusCodes." />
        <valueSet value="http://hl7.org/fhir/ValueSet/condition-ver-status|4.0.1">
          <extension url="http://hl7.org/fhir/StructureDefinition/11179-permitted-value-conceptmap">
            <valueCanonical value="http://nictiz.nl/fhir/ConceptMap/ZekerheidsStatusCodelijst_to_ConditionVerificationStatus" />
          </extension>
        </valueSet>
      </binding>
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.3" />
        <comment value="DegreeOfCertainty" />
      </mapping>
    </element>
    <element id="Condition.verificationStatus.coding">
      <path value="Condition.verificationStatus.coding" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="$this" />
        </discriminator>
        <rules value="open" />
      </slicing>
      <min value="1" />
    </element>
    <element id="Condition.verificationStatus.coding:degreeOfCertaintyCodelist">
      <path value="Condition.verificationStatus.coding" />
      <sliceName value="degreeOfCertaintyCodelist" />
      <min value="1" />
      <max value="1" />
      <binding>
        <strength value="required" />
        <description value="In addition to a coding from this ValueSet, the corresponding coding from the FHIR base ValueSet SHALL be communicated. The ConceptMap &lt;http://nictiz.nl/fhir/ConceptMap/ZekerheidsStatusCodelijst-to-ConditionVerificationStatus&gt; can be used to relate these two ValueSets." />
        <valueSet value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.40.2.99.99.9--20240101000000" />
      </binding>
    </element>
    <element id="Condition.category">
      <path value="Condition.category" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="$this" />
        </discriminator>
        <rules value="open" />
      </slicing>
      <min value="1" />
    </element>
    <element id="Condition.category:diagnosisCode">
      <path value="Condition.category" />
      <sliceName value="diagnosisCode" />
      <min value="1" />
      <max value="1" />
      <patternCodeableConcept>
        <coding>
          <system value="http://snomed.info/sct" />
          <code value="439401001" />
        </coding>
      </patternCodeableConcept>
    </element>
    <element id="Condition.category:complicationCode">
      <path value="Condition.category" />
      <sliceName value="complicationCode" />
      <comment value="If the diagnostic insight involves a complication (e.g. IsComplication = _true_), this `.category` SHALL be present." />
      <max value="1" />
      <patternCodeableConcept>
        <coding>
          <system value="http://snomed.info/sct" />
          <code value="116223007" />
        </coding>
      </patternCodeableConcept>
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.6" />
        <comment value="IsComplication (implicit; this category corresponds to IsComplication = true)" />
      </mapping>
    </element>
    <element id="Condition.code">
      <path value="Condition.code" />
      <short value="DiagnosisName" />
      <definition value="The term with associated code that the care professional selects from the used code system with conditions." />
      <comment value="The zib DiagnosticInsight possibly includes multiple Diagnoses, while a Condition resource can only contain one `.code`. Whenever multiple Diagnoses are present (i.e. the DiagnosticInsight represents a differential diagnosis), each Diagnosis results in a separate instance of this profile, with `.verificationStatus` set to _differential_ and all other concepts from the zib duplicated in each of these instances.&#xD;&#xA;&#xD;&#xA;Note that a receiving system can determine which Condition instances represent Diagnoses within the same zib DiagnosticInsight instance by considering all Conditions with `.verificationStatus` = _differential_ and checking which of these have the same combination of DiagnosticInsightDate (`.extension:diagnosticInsightDate.value`), Diagnostician (`.asserter`) and Condition (`.extension:condition.value`)." />
      <alias value="DiagnoseNaam" />
      <binding>
        <strength value="required" />
        <valueSet value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.40.2.99.99.11--20240101000000" />
      </binding>
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.14" />
        <comment value="DiagnosisName" />
      </mapping>
    </element>
    <element id="Condition.code.extension:furtherSpecificationDiagnosisName">
      <path value="Condition.code.extension" />
      <sliceName value="furtherSpecificationDiagnosisName" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://nictiz.nl/fhir/StructureDefinition/ext-DiagnosticInsight.FurtherSpecificationDiagnosisName" />
      </type>
    </element>
    <element id="Condition.bodySite">
      <path value="Condition.bodySite" />
      <short value="AnatomicalLocation" />
      <definition value="The location(s) on and/or in the body that is/are affected by the condition, conform the diagnostics." />
      <alias value="AnatomischeLocatie" />
      <type>
        <code value="CodeableConcept" />
        <profile value="http://nictiz.nl/fhir/StructureDefinition/zib-AnatomicalLocation" />
      </type>
      <binding>
        <strength value="required" />
        <valueSet value="http://decor.nictiz.nl/fhir/ValueSet/2.16.840.1.113883.2.4.3.11.60.121.11.11--20200901000000" />
      </binding>
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.4" />
        <comment value="AnatomicalLocation" />
      </mapping>
    </element>
    <element id="Condition.asserter">
      <path value="Condition.asserter" />
      <short value="Diagnostician" />
      <definition value="The care professional that acquired the diagnostic insight. This can be a different individual than the person who recorded the diagnostic insight." />
      <alias value="DiagnoseSteller" />
      <type>
        <code value="Reference" />
        <profile value="http://nictiz.nl/fhir/StructureDefinition/pattern-ZibHealthProfessionalReference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Practitioner" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/PractitionerRole" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Patient" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/RelatedPerson" />
        <targetProfile value="http://nictiz.nl/fhir/StructureDefinition/zib-HealthProfessional-PractitionerRole" />
      </type>
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.11" />
        <comment value="Diagnostician" />
      </mapping>
    </element>
    <element id="Condition.note">
      <path value="Condition.note" />
      <max value="1" />
    </element>
    <element id="Condition.note.text">
      <path value="Condition.note.text" />
      <short value="Comment" />
      <definition value="A comment in free text with respect to the diagnostic insight, that is not represented by the other data elements in the information model." />
      <alias value="Toelichting" />
      <mapping>
        <identity value="zib-diagnosticinsight-v1.0-2023EN" />
        <map value="NL-CM:99.99.5" />
        <comment value="Comment" />
      </mapping>
    </element>
  </differential>
</StructureDefinition>